{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Endpoints": {
    "QueryLimiter": "VAR_ENDPOINTS_QUERY_LIMITER"
  },
  "Security": {
    "EncryptionSecret": "VAR_SECURITY_ENCRYPTION_SECRET"
  },
  "ConnectionStrings": {
    "Provider": "VAR_CONNECTION_PROVIDER_NAME",
    "Default": "VAR_CONNECTION_STRINGS"
  },
  "FakeDetector": {
    "ApiKey": "VAR_FAKE_DETECTOR_KEY",
    "Uri": "VAR_FAKE_DETECTOR_URI"
  }
}
