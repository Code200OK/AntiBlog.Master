{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "DevUI": {
    "ProxyUrl": "http://localhost:4200"
  },
  "Swagger": {
    "StaticRoute": "/swagger/v1.2/swagger.json"
  },
  "Endpoints": {
    "QueryItemsLimit": 20
  },
  "Security": {
    "EndpointEncryptionSecret": "development security key"
  },
  "ConnectionStrings": {
    "Default": "Data Source=(LocalDB)\\MSSQLLocalDB;Database=CampusDb;Integrated Security=True;MultipleActiveResultSets=True"
  }
}
