{
  "name": "cognibuild.campus",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ci": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build",
    "build:cd": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod",
    "build:watch": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --watch",
    "test": "jest",
    "test:ci": "node --max_old_space_size=8192 ./node_modules/jest/bin/jest --esModuleInterop=true",
    "lint": "ng lint",
    "lint:ci": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng lint",
    "e2e": "ng e2e",
    "test:watch": "node --max_old_space_size=8192 ./node_modules/jest/bin/jest --esModuleInterop=true --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/forms": "~9.1.0",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@briebug/jest-schematic": "^2.2.0",
    "angular-validation-match": "^1.9.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^26.0.10",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^26.4.2",
    "jest-preset-angular": "^8.3.1",
    "protractor": "~5.4.3",
